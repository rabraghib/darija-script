{
  "Print Statement": {
    "prefix": "golih",
    "body": ["golih(${1:\"Hello, World!\"});"],
    "description": "Prints 'Hello, World!' to the console"
  },
  "Variable Declaration": {
    "prefix": "9ayed",
    "body": ["9ayed ${1:myVariable} = ${2:0};"],
    "description": "Creates a new variable named 'myVariable'"
  },
  "Function Declaration": {
    "prefix": "fonksyon",
    "body": ["fonksyon ${1:myFunction}() {", "\t${2:// body...}", "}"],
    "description": "Creates a new function named 'myFunction'"
  },
  "If Statement": {
    "prefix": "ilakan",
    "body": ["ilakan (${1:true}) {", "\t${2:// body...}", "}"],
    "description": "Creates a new if statement"
  },
  "Else Statement": {
    "prefix": "sinn",
    "body": ["sinn {", "\t${1:// body...}", "}"],
    "description": "Creates a new else statement"
  },
  "If Not Statement": {
    "prefix": "ilamakanch",
    "body": ["ilamakanch (${1:false}) {", "\t${2:// body...}", "}"],
    "description": "Creates a new if not statement"
  },
  "While Loop": {
    "prefix": "ma7dBa9i",
    "body": ["ma7dBa9i (${1:true}) {", "\t${2:// body...}", "}"],
    "description": "Creates a new while loop"
  },
  "For Loop": {
    "prefix": "ma7dBa9i",
    "body": [
      "9ayed ${1:i} = ${2:0};",
      "ma7dBa9i (${1:i} < ${3:10}) {",
      "\t${4:// body...}",
      "\t${1:i} = ${1:i} + 1;",
      "}"
    ],
    "description": "Creates a new for loop"
  },
  "Input Statement": {
    "prefix": "dakhel",
    "body": ["9ayed ${1:myVariable} = dakhel(\"${2:Enter input}\");"],
    "description": "Creates a new input statement"
  }
}
