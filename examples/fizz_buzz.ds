# FizzBuzz

fizzBuzz(100);

fonksyon fizzBuzz(n) {
    9ayed i = 0;
    ma7dBa9i(i < n) {
        i = i + 1;
        9ayed result = "";
        ilakan (i % 3 == 0) {
            result = result + "Fizz";
        }
        ilakan (i % 5 == 0) {
            result = result + "Buzz";
        }
        ilamakanch (result == "") {
            golih(result);
        } sinn {
            golih(i);
        }
    }
}
